# .secrets.example - Template for sensitive environment variables
# Copy this file to .secrets and fill in your actual values
# The .secrets file should be added to .gitignore to prevent committing secrets

# Vault secrets
export VAULT_TOKEN="your-vault-token-here"

# Keycloak secrets
export KEYCLOAK_PASSWORD="your-keycloak-password"

# Database secrets
export POSTGRES_PASSWORD="your-postgres-password"

# Minio secrets
export MINIO_ROOT_PASSWORD="your-minio-password"

# GitHub tokens (if needed)
export GITHUB_TOKEN="your-github-token"

# Docker registry credentials (if needed)
export DOCKER_REGISTRY_PASSWORD="your-registry-password"

# GitHub App
export GITHUB_ACCOUNT=<your-account-slug> # eg thatmlopsguy
export GITHUB_APP_KEY_PATH=<path-to-the-generate-private-key>
export GITHUB_APP_ID=<your-github-app-id>
# https://github.com/settings/installations/<your-github-app-installation-id>
export GITHUB_APP_INSTALLATION_ID=<your-github-app-installation-id>
